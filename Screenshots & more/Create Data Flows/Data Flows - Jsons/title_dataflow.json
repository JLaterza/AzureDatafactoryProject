{
    "name": "title_dataflow",
    "properties": {
        "type": "MappingDataFlow",
        "typeProperties": {
            "sources": [
                {
                    "dataset": {
                        "referenceName": "TitleMasterSource",
                        "type": "DatasetReference"
                    },
                    "name": "TitleSource"
                }
            ],
            "sinks": [
                {
                    "dataset": {
                        "referenceName": "NycPayrollTitleMDTarget",
                        "type": "DatasetReference"
                    },
                    "name": "TitleTarget"
                }
            ],
            "transformations": [],
            "scriptLines": [
                "source(output(",
                "          TitleCode as string,",
                "          TitleDescription as string",
                "     ),",
                "     allowSchemaDrift: true,",
                "     validateSchema: false,",
                "     ignoreNoFilesFound: false) ~> TitleSource",
                "TitleSource sink(allowSchemaDrift: true,",
                "     validateSchema: false,",
                "     input(",
                "          TitleCode as string,",
                "          TitleDescription as string",
                "     ),",
                "     deletable:false,",
                "     insertable:true,",
                "     updateable:false,",
                "     upsertable:false,",
                "     format: 'table',",
                "     skipDuplicateMapInputs: true,",
                "     skipDuplicateMapOutputs: true,",
                "     errorHandlingOption: 'stopOnFirstError') ~> TitleTarget"
            ]
        }
    }
}